{
  "furnace_input_panel/panel": {
    "modifications": [
      {
        "array_name": "controls",
        "operation": "replace",
        "where": {
          "furnace_fuel_panel@furnace_fuel_panel": {}
        },
        "value": {
          "furnace_fuel_panel@furnace_fuel_panel": {
            "$tmp_title": "$container_title",
            "visible": "(($tmp_title - 'ui.techdawn') = $tmp_title)",
            "layer": 0
          }
        }
      },
      {
        "array_name": "controls",
        "operation": "replace",
        "where": {
          "flame_panel@flame_panel": {}
        },
        "value": {
          "flame_panel@flame_panel": {
            "controls": [
              {
                "flame_empty_image@furnace.flame_empty_image": {
                  "ignored": "(not (($container_title - 'ui.techdawn') = $container_title))"
                }
              },
              {
                "flame_full_image@furnace.flame_full_image": {
                  "ignored": "(not (($container_title - 'ui.techdawn') = $container_title))"
                }
              },
              {
                "flame_empty_image@furnace.flame_empty_image": {
                  "texture": "textures/ui/battery_empty",
                  "ignored": "(($container_title - 'ui.techdawn') = $container_title)"
                }
              },
              {
                "flame_full_image@furnace.flame_full_image": {
                  "texture": "textures/ui/battery_full",
                  "ignored": "(($container_title - 'ui.techdawn') = $container_title)"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "furnace_input_panel": {
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "variables": [
      {
        "requires": "(not (($container_title - 'ui.techdawn') = $container_title))",
        "$padding_furnace": [0, 14]
      },
      {
        "requires": "(($container_title - 'ui.techdawn') = $container_title)",
        "$padding_furnace": [0, 0]
      }
    ],
    "offset": "$padding_furnace"
  }
}